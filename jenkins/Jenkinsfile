pipeline {
    agent any
    tools{
        maven 'M2_HOME'
    }
    stages {
        stage('Build') {
            steps {
                sh 'pwd'
                sh 'mvn -B -DskipTests clean package'
            }
        }
        stage('Install') {
            steps {
                sh 'pwd'
                sh 'cd sunway-cloud-user'
                sh 'pwd'
                sh 'mvn -B -DskipTests clean package'
            }
        }
    }
}
